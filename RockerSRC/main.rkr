include "RockerSRC/stdlib.rkr"
include "RockerSRC/lexer.rkr"
include "RockerSRC/parser.rkr"

//-----------------------------------------------------------------------------
// MAIN FUNCTION
//-----------------------------------------------------------------------------



let main(): int => {
    let l: lexer => new_lexer("RockerSRC/main.rkr");
    lex_program(l);
    print_int(get_list_length(l::Toks));
    print("\n");
    let p: parser => new_parser(l::Toks);
    parse_program(p);
    return 0;
}

