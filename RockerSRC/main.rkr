include "RockerSRC/stdlib.rkr"
include "RockerSRC/lexer.rkr"
include "RockerSRC/parser.rkr"

//-----------------------------------------------------------------------------
// MAIN FUNCTION
//-----------------------------------------------------------------------------



let main(): void => {
    let l: lexer => new_lexer("RockerSRC/main.rkr");
    lex_program(l);
    let toks: token[] => l::Toks;
    print_list_types(toks);
    let p: parser => new_parser(toks);
    parse_program(p);
}

