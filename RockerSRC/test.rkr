let is_delimeter(c: char): boolean => {
    let delimeters: string => "><;,:-+*/%&|!=(){}^";
    let len: int => get_string_length(delimeters);
    loop i: 0 -> len - 1 => {
        if get_nth_char(delimeters, i) = c then {
            return true;
        }
    }
    if c = '\'' || c = '\"' then return true;
    return false;
}

