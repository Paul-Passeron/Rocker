let main : void -> int => {
     let a : void -> int => {let () => print "Hello World !\n" in 0} in
     let () => print "Hello World !\n" in
     let () => {a _ }in a _
}


let fact n : int -> int => {
     match n => {
          -> 0 : 1
          -> 1 : 1
          -> n : n * fact(n - 1)
     }
}