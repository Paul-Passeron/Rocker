rec int_node => {
  Value: int,
  Next: int_linked_list
}
pro int_linked_list => {
  Tail: void,
  Node: int_node
}


let append(l: int_linked_list, val: int): int_linked_list => {
  let node: int_node => {
    Value => val,
    Next => l,
  };
  let res: int_linked_list => Node::(node);
  return res;
} 

